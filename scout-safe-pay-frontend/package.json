{
  "name": "scout-safe-pay-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "AutoScout24 SafeTrade - Frontend (Next.js 16.1.1)",
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "test:live": "playwright test --config=playwright.live.config.ts",
    "test:live:frontend": "playwright test --config=playwright.live.config.ts --project=frontend-chrome",
    "test:live:admin": "playwright test --config=playwright.live.config.ts --project=admin-chrome",
    "test:live:flows": "playwright test --config=playwright.live.config.ts e2e/live/*-flows.spec.ts",
    "test:live:admin-flows": "playwright test --config=playwright.live.config.ts --project=admin-chrome e2e/live/admin-flows.spec.ts",
    "test:live:frontend-flows": "playwright test --config=playwright.live.config.ts --project=frontend-chrome e2e/live/frontend-flows.spec.ts",
    "test:live:e2e-flows": "playwright test --config=playwright.live.config.ts e2e/live/e2e-complete-flows.spec.ts",
    "test:live:headed": "playwright test --config=playwright.live.config.ts --headed",
    "test:live:ui": "playwright test --config=playwright.live.config.ts --ui",
    "test:live:report": "playwright show-report test-results/live-report"
  },
